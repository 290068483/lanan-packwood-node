{
  "name": "xml-parser-optimizer",
  "version": "1.0.0",
  "description": "优化XML解析和处理流程，提高处理效率和数据准确性",
  "main": "src/main.js",
  "scripts": {
    "start": "node start-electron.js",
    "dev": "concurrently \"node src/main.js\" \"npm run electron\" \"node src/ui/server.js\" --names \"MAIN,ELECTRON,SERVER\" --prefix-colors \"green,cyan,yellow\"",
    "test": "jest",
    "electron": "node start-electron.js",
    "electron-dev": "cross-env NODE_ENV=development node start-electron.js",
    "electron-start-url": "cross-env ELECTRON_START_URL=http://localhost:3000 node start-electron.js",
    "electron-pack": "electron-builder --win",
    "electron-dist": "electron-builder --win --publish never",
    "sync": "node src/services/main-service.js"
  },
  "engines": {
    "node": ">=18.0.0 <20.0.0"
  },
  "keywords": [
    "xml",
    "parser",
    "optimizer"
  ],
  "author": "Developer",
  "license": "ISC",
  "dependencies": {
    "@xmldom/xmldom": "^0.8.10",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "chokidar": "^3.5.3",
    "electron": "38.1.0",
    "exceljs": "^4.4.0",
    "fast-xml-parser": "^4.2.5",
    "libxmljs2": "^0.32.0",
    "lowdb": "^7.0.1",
    "node-cron": "^3.0.2",
    "node-gyp": "^11.4.2",
    "xlsx": "^0.18.5",
    "xml-js": "^1.6.11",
    "xml2js": "^0.6.0",
    "xpath": "^0.0.34"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron-builder": "^24.9.1",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.2",
    "windows-build-tools": "^5.2.2"
  }
}
