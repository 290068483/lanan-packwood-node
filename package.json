{
  "name": "xml-parser-optimizer",
  "version": "1.0.0",
  "description": "优化XML解析和处理流程，提高处理效率和数据准确性",
  "main": "src/main.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=production electron src/scripts/start-electron.js",
    "dev": "concurrently \"node src/main.js --dev\" \"npm run electron\" \"node src/ui/server.js\" --names \"MAIN,ELECTRON,SERVER\" --prefix-colors \"green,cyan,yellow\"",
    "dev-ui": "cross-env NODE_ENV=development npm run electron-dev",
    "dev-vue": "vite",
    "build-vue": "vue-tsc && vite build",
    "preview-vue": "vite preview",
    "test": "cross-env NODE_ENV=test jest",
    "test:ui": "cross-env NODE_ENV=test electron src/scripts/start-electron.js",
    "electron": "cross-env NODE_ENV=production electron src/scripts/start-electron.js",
    "electron-dev": "cross-env NODE_ENV=development electron src/scripts/start-electron.js",
    "electron-start-url": "cross-env ELECTRON_START_URL=http://localhost:5173 electron src/scripts/start-electron.js",
    "electron-pack": "electron-builder --win",
    "electron-dist": "electron-builder --win --publish never",
    "sync": "node src/services/main-service.js",
    "start-main": "ts-node -r tsconfig-paths/register src/main.ts"
  },
  "engines": {
    "node": ">=18.0.0 <20.0.0"
  },
  "keywords": [
    "xml",
    "parser",
    "optimizer"
  ],
  "author": "Developer",
  "license": "ISC",
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.0",
    "@xmldom/xmldom": "^0.8.10",
    "archiver": "^7.0.1",
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "chokidar": "^3.5.3",
    "electron": "38.1.0",
    "element-plus": "^2.4.0",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "fast-xml-parser": "^4.2.5",
    "libxmljs2": "^0.32.0",
    "lowdb": "^7.0.1",
    "node-cron": "^3.0.2",
    "node-gyp": "^11.4.2",
    "pinia": "^2.1.0",
    "unzipper": "^0.12.3",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0",
    "xlsx": "^0.18.5",
    "xml-js": "^1.6.11",
    "xml2js": "^0.6.0",
    "xpath": "^0.0.34"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron-builder": "^24.9.1",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.0",
    "jest": "^30.1.3",
    "sass": "^1.69.0",
    "ts-jest": "^29.4.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "windows-build-tools": "^5.2.2"
  }
}
